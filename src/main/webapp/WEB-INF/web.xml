<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>DB Representation Task</display-name>
    <welcome-file-list>
        <welcome-file>/WEB-INF/view/index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>com.dd.service.IndexServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TryOutServlet</servlet-name>
        <servlet-class>com.dd.service.TryOutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TryOutServlet</servlet-name>
        <url-pattern>/tryout</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LastHourServlet</servlet-name>
        <servlet-class>com.dd.service.LastHourServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LastHourServlet</servlet-name>
        <url-pattern>/lasthour</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>IncompleteFormServlet</servlet-name>
        <servlet-class>com.dd.service.IncompleteFormServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IncompleteFormServlet</servlet-name>
        <url-pattern>/incomplete</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TopFormsServlet</servlet-name>
        <servlet-class>com.dd.service.TopFormsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TopFormsServlet</servlet-name>
        <url-pattern>/topforms</url-pattern>
    </servlet-mapping>

</web-app>